{
  "name": "fruityui",
  "private": true,
  "version": "0.0.0",
  "description": "Fruity ui monorepo",
  "author": "Cherednichenko Anton",
  "license": "MIT",
  "type": "module",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "storybook dev -p 6006",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "build": "yarn workspaces foreach --all -pv run build",
    "build:sb": "storybook build",
    "deploy:sb": "gh-pages -d storybook-static",
    "prettier:test": "prettier --check \"**/*.{ts,tsx,css}\"",
    "prettier:write": "prettier --write \"**/*.{ts,tsx,css}\""
  },
  "dependencies": {
    "react": "^19.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/plugin-transform-runtime": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.27.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.3",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@storybook/addon-actions": "^8.5.3",
    "@storybook/addon-essentials": "^8.5.3",
    "@storybook/blocks": "^8.5.3",
    "@storybook/builder-webpack5": "^8.6.12",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/react": "^8.6.10",
    "@storybook/react-webpack5": "^8.6.12",
    "@storybook/test": "^8.6.12",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/babel__core": "^7",
    "@types/babel__plugin-transform-runtime": "^7",
    "@types/babel__preset-env": "^7",
    "@types/node": "^22.15.2",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@types/rollup-plugin-peer-deps-external": "^2",
    "@typescript-eslint/eslint-plugin": "^8.23.0",
    "@typescript-eslint/parser": "^8.23.0",
    "babel-loader": "^10.0.0",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-storybook": "^0.11.2",
    "gh-pages": "^6.3.0",
    "jsdom": "^26.0.0",
    "postcss": "^8.5.3",
    "prettier": "^3.4.2",
    "react-dom": "^19.0.0",
    "rollup": "^4.40.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "storybook": "^8.5.3",
    "ts-loader": "^9.5.2",
    "tsconfig-paths-webpack-plugin": "^4.2.0",
    "typescript": "^5.8.3"
  },
  "packageManager": "yarn@4.8.1"
}
